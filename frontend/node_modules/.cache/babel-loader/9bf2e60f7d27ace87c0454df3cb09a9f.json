urceRoot":"src/components","sources":["Users.vue"],"sourcesContent":["<template>\n  <div class=\"container\">\n    <h3>All Users</h3>\n    <div v-if=\"message\" class=\"alert alert-success\">\n      {{ this.message }}</div>\n    <div class=\"container\">\n      <table class=\"table\">\n        <thead>\n        <tr>\n          <th>First Name</th>\n          <th>Last Name</th>\n          <th>Email</th>\n          <th>Update</th>\n          <th>Delete</th>\n        </tr>\n        </thead>\n        <tbody>\n        <tr v-for=\"user in users\" v-bind:key=\"user.id\">\n\n          <td>{{ user.firstName }}</td>\n          <td>{{ user.lastName }}</td>\n          <td>{{ user.email }}</td>\n          <td>\n            <button class=\"btn btn-warning\"\n                    v-on:click=\"updateUser(user.id)\">\n              Update\n            </button>\n          </td>\n          <td>\n            <button class=\"btn btn-danger\"\n                    v-on:click=\"deleteUser(user.id)\">\n              Delete\n            </button>\n          </td>\n        </tr>\n        </tbody>\n      </table>\n      <div class=\"row\">\n        <button class=\"btn btn-success\"\n                v-on:click=\"addUser()\">Add</button>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport UserDataService from \"../service/UserDataService\";\nexport default {\n  name: \"Users\",\n  data() {\n    return {\n      users: [],\n      message: \"\",\n    };\n  },\n  methods: {\n    refreshUsers() {\n      UserDataService.retrieveAllUsers().then((res) => {\n        this.users = res.data;\n      });\n    },\n    addUser() {\n      this.$router.push(`/user/-1`);\n    },\n    updateUser(id) {\n      this.$router.push(`/user/${id}`);\n    },\n    deleteUser(id) {\n      UserDataService.deleteUser(id).then(() => {\n        this.refreshUsers();\n      });\n    },\n  },\n  created() {\n    this.refreshUsers();\n  },\n};\n</script>"]}]}